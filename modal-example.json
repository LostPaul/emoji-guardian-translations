{
    "modal": {
        "title": "Server report form",
        "custom_id": "[\"report server\", \"{placeholder}\"]",
        "components": [
            {
                "type": 1,
                "components": [
                    {
                        "type": 4,
                        "custom_id": "serverid",
                        "label": "Server ID",
                        "style": 1,
                        "min_length": 10,
                        "max_length": 24,
                        "placeholder": "Insert the ID from the server ID here.",
                        "required": true
                    }
                ]
            },
            {
                "type": 1,
                "components": [
                    {
                        "type": 4,
                        "custom_id": "reason",
                        "label": "The reason for the report",
                        "style": 2,
                        "min_length": 30,
                        "max_length": 1000,
                        "placeholder": "Insert the reason for your report here.",
                        "required": true
                    }
                ]
            },
            {
                "type": 1,
                "components": [
                    {
                        "type": 4,
                        "custom_id": "server_invite",
                        "label": "Invite link for the server",
                        "style": 2,
                        "min_length": 15,
                        "max_length": 40,
                        "placeholder": "Insert the invite to the server here.",
                        "required": false
                    }
                ]
            },
            {
                "type": 1,
                "components": [
                    {
                        "type": 4,
                        "custom_id": "image",
                        "label": "Image url related to the report",
                        "style": 2,
                        "min_length": 10,
                        "max_length": 500,
                        "placeholder": "Insert images related to the report here.",
                        "required": false
                    }
                ]
            }
        ]
    }
}