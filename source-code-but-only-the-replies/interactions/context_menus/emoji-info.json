{
    "nothing": "client.language.get(locale, 'emoji-info', 'nothing')",
    "true": "client.language.get(locale, 'emoji-info', 'true')",
    "false": "client.language.get(locale, 'emoji-info', 'false')",
    "embed": {
        "color": "2f3136",
        "title": "client.language.get(locale, 'emoji-info', 'embed.title')", 
        "thumbnail": {
            "url": "{current_emoji_url}"
        },
        "fields": [
            {
                "name": "client.language.get(locale, 'emoji-info', 'embed.fields', 'array[0].name')",
                "value": "{current_emoji_name}",
                "inline": true
            },
            {
                "name": "client.language.get(locale, 'emoji-info', 'embed.fields', 'array[1].name')",
                "value": "{current_emoji_id}",
                "inline": true
            },
            {
                "name": "client.language.get(locale, 'emoji-info', 'embed.fields', 'array[2].name')",
                "value": "{current_emoji_managed}",
                "inline": true
            },
            {
                "name": "client.language.get(locale, 'emoji-info', 'embed.fields', 'array[3].name')",
                "value": "{current_emoji_roles}",
                "inline": false
            },
            {
                "name": "client.language.get(locale, 'emoji-info', 'embed.fields', 'array[4].name')",
                "value": "<t:{current_emoji_creation_timestamp}:D>",
                "inline": true
            },
            {
                "name": "client.language.get(locale, 'emoji-info', 'embed.fields', 'array[5].name')",
                "value": "{current_emoji_author}",
                "inline": true
            },
            {
                "name": "client.language.get(locale, 'emoji-info', 'embed.fields', 'array[6].name')",
                "value": "[Click here]({current_emoji_url})",
                "inline": true
            }
        ]
    }
}