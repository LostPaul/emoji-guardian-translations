{
    "modal": {
        "title": "User report form",
        "custom_id": "[\"report user\", \"{placeholder}\"]",
        "components": [
            {
                "type": 1,
                "components": [
                    {
                        "type": 4,
                        "custom_id": "userid",
                        "label": "User ID",
                        "style": 1,
                        "min_length": 10,
                        "max_length": 24,
                        "placeholder": "Insert the ID from the user ID here.",
                        "required": true
                    }
                ]
            },
            {
                "type": 1,
                "components": [
                    {
                        "type": 4,
                        "custom_id": "reason",
                        "label": "The reason for the report",
                        "style": 2,
                        "min_length": 30,
                        "max_length": 1000,
                        "placeholder": "Insert the reason for your report here.",
                        "required": true
                    }
                ]
            },
            {
                "type": 1,
                "components": [
                    {
                        "type": 4,
                        "custom_id": "image",
                        "label": "Image url related to the report",
                        "style": 2,
                        "min_length": 10,
                        "max_length": 500,
                        "placeholder": "Insert images related to the report here.",
                        "required": false
                    }
                ]
            }
        ]
    }
}